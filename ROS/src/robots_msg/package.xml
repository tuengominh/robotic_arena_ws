<?xml version="1.0"?>
<package format="2">
  <name>robots_msg</name>
  <version>0.0.0</version>
  <description>The ROS message package</description>
  <maintainer email="aescuredo@ibecbarcelona.eu">aescuredo</maintainer>
  <license>BSD</license>
  
  <buildtool_depend>catkin</buildtool_depend>
  <buildtool_depend>genmsg</buildtool_depend>
