  <depend>eco_msgs.msg</depend>
  <depend>tanke_msgs.msg</depend>
  <depend>armpap_msgs.msg</depend>
  <depend>jevois.msg</depend>
  <depend>lidar.msg</depend>
  <depend>camera.msg</depend>
  <depend>motor.msg</depend>
  <depend>plat.msg</depend>
  <depend>target.msg</depend>
